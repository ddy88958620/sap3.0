package com.hcicloud.sap.model.quality;

import java.util.Date;

import com.hcicloud.sap.model.admin.User;

/**
 * 自动质检规则
 * @author wangya
 *
 */
public class AutoRule{
	//主键ID
	private String uuid;
	//规则名称
	private String name;
	//规则内容
	private String content;
	//最后更新用户
	private User updateUser;
	//最后更新时间
	private Date updateTime;
	//状态
	private Integer state;
	//备注
	private String remark;
	//规则逻辑
	private String script;
	//所属规则集
	private AutoRules autoRules;
	
	/**
	 * 获取uuid
	 * @return the uuid
	 */
	public String getUuid() {
		return uuid;
	}
	/**
	 * 设置uuid
	 * @param uuid the uuid to set
	 */
	public void setUuid(String uuid) {
		this.uuid = uuid;
	}
	/**
	 * 获取name
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * 设置name
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * 获取content
	 * @return the content
	 */
	public String getContent() {
		return content;
	}
	/**
	 * 设置content
	 * @param content the content to set
	 */
	public void setContent(String content) {
		this.content = content;
	}
	/**
	 * 获取updateUser
	 * @return the updateUser
	 */
	public User getUpdateUser() {
		return updateUser;
	}
	/**
	 * 设置updateUser
	 * @param updateUser the updateUser to set
	 */
	public void setUpdateUser(User updateUser) {
		this.updateUser = updateUser;
	}
	/**
	 * 获取updateTime
	 * @return the updateTime
	 */
	public Date getUpdateTime() {
		return updateTime;
	}
	/**
	 * 设置updateTime
	 * @param updateTime the updateTime to set
	 */
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	/**
	 * 获取state
	 * @return the state
	 */
	public Integer getState() {
		return state;
	}
	/**
	 * 设置state
	 * @param state the state to set
	 */
	public void setState(Integer state) {
		this.state = state;
	}
	/**
	 * 获取remark
	 * @return the remark
	 */
	public String getRemark() {
		return remark;
	}
	/**
	 * 设置remark
	 * @param remark the remark to set
	 */
	public void setRemark(String remark) {
		this.remark = remark;
	}
	/**
	 * 获取script
	 * @return the script
	 */
	public String getScript() {
		return script;
	}
	/**
	 * 设置script
	 * @param script the script to set
	 */
	public void setScript(String script) {
		this.script = script;
	}
	/**
	 * 获取autoRules
	 * @return the autoRules
	 */
	public AutoRules getAutoRules() {
		return autoRules;
	}
	/**
	 * 设置autoRules
	 * @param autoRulesId the autoRules to set
	 */
	public void setAutoRules(AutoRules autoRules) {
		this.autoRules = autoRules;
	}
	
	
}