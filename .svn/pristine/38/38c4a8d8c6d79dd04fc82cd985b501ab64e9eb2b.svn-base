<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hcicloud.sap.model.admin.UserRoleRelation" table="sys_user_role_relation" >
		 <composite-id>
            <!-- key-property标签表示哪一些属性对应复合主键 -->
            <key-property name="userId" column="user_id"></key-property>
            <key-property name="roleId" column="role_id"></key-property>
        </composite-id>
    </class>
</hibernate-mapping>
